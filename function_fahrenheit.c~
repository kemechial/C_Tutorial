#include <stdio.h>

#define LOWER 0
#define UPPER 300
#define STEP 20

float fahr_2_celc(int);

main()
{
 int fahr; 
  for(fahr=LOWER;fahr<= UPPER;fahr=fahr+STEP)
    printf("%3d\t%6.1f\n", fahr, (5.0/9.0)*(fahr-32));
  printf("end of program\n");
}

float fahr_2_celc(int fahr){
   return (5.0/9.0)*(fahr-32);
}
